    "styles": { 
       "page": {
            "--bg-colors": ["#900048", "#3D0D64", "#240041"],
            "--gradient-angle": "180deg",
            "minHeight": "100vh",
            "display": "flex",
            "flexDirection": "column",
            "alignItems": "center",
            "justifyContent": "center",
            "background": "linear-gradient(var(--gradient-angle), var(--bg-colors))"
        },
        "profile": {
            "maxWidth": "300px", 
            "borderRadius": "20%"
        },
        "profile-text": {
            "color": "#ffffff"
        },
        "section": {
            "color": "#ffffff"
        },
        "panel-details": {
            "boxSizing": "border-box",
            "background": "#00ffb7",
            "borderRadius": "16px",
            "height": 48,
            "padding": "0 30px",
            "boxShadow": "0 3px 5px 2px rgba(255, 105, 135, .3)",
            "minWidth": "500px",
            "minHeight": "60px"
        },
        "content": {
            "margin": "12px 0",
            "display": "flex",
            "transition": "margin 150ms cubic-bezier(0.4, 0, 0.2, 1) 0ms",
            "alignContent": "center",
            "alignItems": "center",
            "justifyContent": "flex-start"
        },
        "panel": {
            "boxSizing": "border-box",
            "borderRadius": "16px",
            "boxShadow": "0 3px 5px 2px rgba(255, 105, 135, .3)",
            "fontSize": "0.875rem",
            "&:last-child": {
                "borderRadius": "16px"
            }
        },
        "button": {
            "background": "#00ffb7",
            "borderRadius": 16,
            "border": 0,
            "color": "#000000",
            "height": 48,
            "padding": "0 30px",
            "boxShadow": "0 3px 5px 2px rgba(255, 105, 135, .3)",
            "minWidth": "500px",
            "minHeight": "60px",
            "textTransform": "capitalize"
        },
        "startIcon": {
            "flexBasis": "40%",
            "display": "flex",
            "alignItems": "center",
            "fontSize": "3em"
        },
        "iconSizeMedium": {
            "fontSize": "45px",
            "*:first-child": {
                "fontSize": "45px"
            }
        },
        "label": {
            "fontSize": "1rem",
            "display": "flex",
            "justifyContent": "flex-start"
        }
        
    }
}