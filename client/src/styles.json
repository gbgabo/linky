{
   "RawStyles": 
   { 
       "page": {
            "--bg-colors": ["#900048", "#240041"],
            "--gradient-angle": "90deg",
            "minHeight": "100vh",
            "display": "flex",
            "flexDirection": "column",
            "alignItems": "center",
            "justifyContent": "center",
            "background": "linear-gradient(var(--gradient-angle), var(--bg-colors))"
        },
        "profile": {
            "marginBottom": "50px", 
            "maxWidth": "300px", 
            "borderRadius": "50%"
        },
        "button": {
            "background": "#00ffb7",
            "borderRadius": 50,
            "border": 0,
            "color": "#000000",
            "height": 48,
            "padding": "0 30px",
            "boxShadow": "0 3px 5px 2px rgba(255, 105, 135, .3)",
            "marginTop": "20px",
            "minWidth": "300px"
        }
    }
  }