  "theme": {
    "page": {
      "--bg-colors": ["#900048", "#240041", "#19002E"],
      "--gradient-angle": "180deg",
      "height": "100%",
      "display": "flex",
      "flexDirection": "column",
      "alignItems": "center",
      "justifyContent": "center",
      "background": "linear-gradient(var(--gradient-angle), var(--bg-colors))",
      "backgroundSize": "cover",
      "backgroundPosition": "center",
      "backgroundRepeat": "no-repeat",
      "width": "100%",
      "position": "absolute",
      "zIndex": 1,
      "top": "0",
      "overflow-x": "hidden"
    },
    "profile": {
      "maxWidth": "12rem",
      "borderRadius": "20%"
    },
    "profile-text": {
      "color": "#00ffb7",
      "textTransform": "lowercase",
      "fontFamily": "'JetBrains Mono', monospace"
    },
    "outlined": {
      "background": "rgb(0 255 183 / 21%)",
      "borderColor": "#00ffb7"
    },
    "section": {
      "color": "#ffffff",
      "fontFamily": "'JetBrains Mono', monospace"
    },
    "iconLink": {
      "color": "#00ffb7"
    },
    "panel": {
      "--border-radius": "30px",
      "minWidth": "20rem",
      "maxWidth": "120px",
      "boxSizing": "border-box",
      "color": "#00ffb7",
      "borderRadius": "var(--border-radius)",
      "fontSize": "1rem",
      "background": "#000000",
      "&:last-child": {
        "borderRadius": "var(--border-radius)"
      }
    },
    "label": {
      "margin": "12px 0",
      "display": "flex",
      "transition": "margin 150ms cubic-bezier(0.4, 0, 0.2, 1) 0ms",
      "alignContent": "center",
      "alignItems": "center",
      "justifyContent": "center",
      "fontSize": "1rem"
    },
    "button": {
      "boxSizing": "border-box",
      "background": "#900048",
      "fontWeight": 700,
      "borderRadius": "8px",
      "border": 0,
      "color": "#00ffb7",
      "boxShadow": "0 3px 5px 2px #000000",
      "height": 60,
      "width": "20rem",
      "minHeight": "60px",
      "textTransform": "inherit",
      "alignItems": "center",
      "fontFamily": "'JetBrains Mono', monospace"
    },
    "icon": {
      "position": "absolute",
      "left": "5%",
      "fontSize": "1rem",
      "display": "flex"
    },
    "expandedIcon": {
      "position": "absolute",
      "right": "5%",
      "fontSize": "1rem",
      "display": "flex",
      "flexBasis": "flex-start"
    }
  }
